{
  "name": "starter-webpack",
  "version": "1.0.0",
  "description": "A boilerplate for HTML+CSS+JS with Webpack. Out-of-the-box upport for Typescript, Sass, JSX.",
  "main": "index.js",
  "scripts": {
    "dev:server": "webpack serve",
    "dev:server-open": "webpack serve --open",
    "build": "rm -rf ./dist && webpack --config webpack.config.production.js",
    "dev:assets": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tresorama/starter-webpack.git"
  },
  "keywords": [
    "starter",
    "webpack",
    "typescript",
    "jsx",
    "sass"
  ],
  "author": "Jacopo Marrone",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tresorama/starter-webpack/issues"
  },
  "homepage": "https://github.com/tresorama/starter-webpack#readme",
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.25.2",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "regenerator-runtime": "^0.13.9",
    "sass": "^1.55.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
